{
    "version": 2,
    "builds": [
        {
            "src": "Dockerfile",
            "use": "@vercel/docker",
            "config": {
                "outputDirectory": "packages/frontend/public"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}